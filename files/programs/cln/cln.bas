10 "C": CLEAR : WAIT 118: PRINT "CALENDAR V.1.6"
20 DIM M(12)
30 INPUT "YEAR:";Y,"MONTH:";J,"DAY:";DN
40 M(1)=0:M(2)=31:M(3)=59:M(4)=90:M(5)=120:M(6)=151:M(7)=181:M(8)=212:M(9)=243
50 M(10)=273:M(11)=304:M(12)=334
60 IF INT (Y/4)=Y/4 AND J>1 LET M(J)=M(J)+1
70 D=((Y-1)*365+INT ((Y-1)/4))+M(J)+DN
100 A=(D/7-INT (D/7))*7
110 IF A<.5 PRINT "SATURDAY": WAIT : END
120 IF A<1 PRINT "SUNDAY": WAIT : END
130 IF A<2 PRINT "MONDAY": WAIT : END
140 IF A<3 PRINT "TUESDAY": WAIT : END
150 IF A<4 PRINT "WEDNESDAY": WAIT : END
160 IF A<5 PRINT "THURSDAY": WAIT : END
170 IF A<6 PRINT "FRIDAY": WAIT : END
